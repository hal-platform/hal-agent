namespace QL\Hal\Agent\Bootstrap;

$root = __DIR__ . '/../';
require_once $root . '/configuration/bootstrap.php';

$file = $root . 'src/CachedContainer.php';
$class = 'QL\Hal\Agent\CachedContainer';
$container = buildDi($root);

file_put_contents($file, dumpDi($container, $class));

echo <<<OUTPUT
Dumping DI Container cache:
File: $file
Class: $class

OUTPUT;
