#!/usr/bin/env bash

START=$(php -r "echo microtime(true);")

cd /Users/skluck/Projects/hal-agent/.build/hal9000-build-9804c64bd5240b21b4cf

OUTPUT=$(tar -vczf /Users/skluck/Projects/hal-agent/.archive/hal9000-build-qlms2.tar.gz . 2>&1)

END=$(php -r "echo microtime(true);")
ELAPSED=$(php -r "echo round($END - $START, 3);")

echo "Success? in $ELAPSED seconds"
